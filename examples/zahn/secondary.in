# The host star, the sun -> Primary body!
sName		secondary		# Body's name
saModules       stellar eqtide binary 

# Physical Parameters
iBodyType     1 # Star!
dMass         1.0 # Mass, solar masses
#dRadius       0.005
dRotPeriod    -1.67 #0.007
dRadGyra      0.5             # Radius of gyration (moment of inertia constant)
dObliquity    0.0

# Orbital properties
dOrbPeriod  -5.0
dEcc        0.1

# EQTIDE Parameters
dTidalQ		1.0e7		# Tidal phase lag
dK2		0.5		# Love number of degree 2
dMaxLockDiff    0.01
saTidePerts	primary   	# Body name(s) of tidal perturbers

# Stellar param
sStellarModel baraffe
sWindModel    reiners

saOutputOrder	Time -TotEn -TotAngMom -Semim -Radius -OrbPer -RotPer Ecce -RotRate -MeanMotion 
