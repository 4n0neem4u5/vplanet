# The host star, the sun -> Primary body!
sName		secondary		# Body's name
saModules       stellar eqtide 

# Physical Parameters
iBodyType     1 # Star!
dMass         1.0 # Mass, solar masses
dRotPeriod    -0.2
dRadGyra      0.27             # Radius of gyration (moment of inertia constant)

# Orbital properties
dOrbPeriod  -4.0
dEcc        0.05

# EQTIDE Parameters
dTidalQ		1.0e6		# Tidal phase lag
dK2		0.5		# Love number of degree 2
dMaxLockDiff    0.01
saTidePerts	primary   	# Body name(s) of tidal perturbers

# Stellar param
sStellarModel baraffe
sMagBrakingModel skumanich

saOutputOrder	Time -TotEn -TotAngMom -Semim -Radius -RotPer Ecce -RotRate -MeanMotion -OrbPer -SurfEnFluxTotal 
