# The host star, the sun -> Primary body!
sName                  primary          # Body's name
saModules              binary stellar

# Physical Parameters
iBodyType              1                # Star!
dMass                  0.3              # Mass, solar masses
dAge                   1.0e6            # Initial age of star
dRotPeriod             -0.5             # Initial rotation period (- -> days)
dRadGyra               0.27             # Radius of gyration (moment of inertia constant)

# STELLAR Parameters
sStellarModel          baraffe          # Specify stellar evolution model (Baraffe+15)
sMagBrakingModel       skumanich        # Specify stellar magnetic braking model

saOutputOrder	Time -Radius -RotPer -RotRate -LostAngMom
