# Host star parameters
sName		      star                  # Body name
saModules    	stellar eqtide        # Active modules

# Physical properties
dMass        	  1.0                 # Mass in solar units
dRotPeriod      -1.0                # Rotation period, negative -> days
dAge            1.0e6               # Initial age [yr]

# STELLAR parameters
sStellarModel  none
sXUVModel      none
dLuminosity    -5e-4      # Negative => solar units
dSatXUVFrac    1e-3       # With no evolution, L_XUV = dLuminosity * dSatXUVFrac

# EQTIDE Parameters
dTidalTau        -0.01      # Tidal time lag (sec)
dK2            0.5        # Love number of degree 2
sTideModel     t8         # Tidal model, p2=CPL, t8=CTL
saTidePerts    auto          # Body name(s) of tidal perturbers
