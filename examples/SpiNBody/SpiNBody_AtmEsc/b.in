# Evaporating mini-Neptune
sName		b		# Body's name
saModules	spinbody atmesc

# Physical Properties
dMass		-30		# Mass, negative -> Earth masses
dRadius		-6		# Radius, negative -> Earth radii
dRotPeriod	-1.0  		# Rotation period, negative -> days
dObliquity	23.5			
dRadGyra	0.5		# Radius of gyration (moment of inertia constant)

# Orbital Properties
bUseOrbParams   1
dEcc            0.4			# Eccentricity
dSemi           0.03			# Semi-major axis, negative -> AU
dInc		3
dLongA		33
dLongP		100
dMeanA          1.2

# ATMESC inputs
dXFrac		1
dAtmXAbsEffH	0.3
dEnvelopeMass	-20

# Output
saOutputOrder Time Semim Ecce Inc LongP LongA -SurfWaterMass -Mass
	      
