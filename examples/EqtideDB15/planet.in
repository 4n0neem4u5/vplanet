# Earthlike parameters
sName		    planet			# Body's name
saModules 	eqtide

sTideModel  DB15

# Physical Properties
dMass		     -1.0  			# Mass, negative -> Earth masses
dRadius		   -1.0  			# Radius, negative -> Earth radii
bForceEqSpin  1
dObliquity	  0
dRadGyra	   0.5
# Orbital Properties
dEcc         0.1		# Eccentricity
dSemi        -0.02		# Semi-major axis, negative -> AU

# EQTIDE Parameters
dTidalQ     100     #
dK2         0.299   # Love number of degree 2 (Yoder 1995)
saTidePerts sun    # Names of perturbing bodies (only central body may have >1)

saOutputOrder -Time -PowerEqtide -SemiMajorAxis Eccentricity -dSemiDt -dEccDtE
