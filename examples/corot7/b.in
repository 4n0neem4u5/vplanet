# corot7-b
sName		b			# Body's name
saModules 	eqtide distorb

# Physical Properties
dMass		-8.0  	        # Mass, negative -> Earth masses
dRadius		-1.58  		# Radius, negative -> Earth radii
dRotPeriod	-1.0  		# Rotation period, negative -> days
dObliquity	0.0             # Obliquity (degrees)
dRadGyra	0.4             # Radius of gyration (unitless)

# Orbital Properties
dEcc            1.0e-5		# Eccentricity
dSemi           0.0188          # Semi-major axis, negative -> AU

# Eqtide Properties
dTidalQ         100             # Tidal phase lag
dK2             0.3             # Love number of degree 2
dMaxLockDiff    0.01
saTidePerts     star          

# DISTORB Parameters
dInc            0.0
dLongA          0.0
dLongP          0.0
sOrbitModel     rd4

#Output
saOutputOrder Time -TotEn -TotAngMom -RotPer LongP SemimajorAxis Eccentricity -SurfEnFluxTot
